{
    "name": "panlatent/site-cli",
    "description": "A command-line tool that help you manage Nginx local development configuration",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "panlatent",
            "email": "panlatent@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "symfony/console": "~3.0",
        "symfony/finder": "~3.0",
        "symfony/filesystem": "~3.0",
        "symfony/process": "^3.3",
        "symfony/yaml": "~3.0",
        "stecman/symfony-console-completion": "^0.7.0",
        "hassankhan/config": "^0.10.0",
        "panlatent/container": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Panlatent\\SiteCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/site-cli"
    ]
}
